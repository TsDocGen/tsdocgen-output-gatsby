{
  "name": "tsdocgen-output-gatsby",
  "version": "0.0.1",
  "description": "Static documentation generator for tsdocgen using gatsby.",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "gatsby:develop": "sh ./bin/develop.sh",
    "gatsby:build": "sh ./bin/build.sh",
    "gatsby:serve": "sh ./bin/serve.sh",
    "gatsby:clean": "sh ./bin/clean.sh",
    "watch": "tsc -w -p"
  },
  "bin": {
    "tsdocgen-gatsby-build": "./dist/bin/build.js",
    "tsdocgen-gatsby-clean": "./bin/clean.sh",
    "tsdocgen-gatsby-develop": "./bin/develop.sh",
    "tsdocgen-gatsby-serve": "./bin/serve.sh"
  },
  "author": "Zerry Hogan",
  "license": "ISC",
  "devDependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@types/glob": "^7.1.4",
    "@types/lodash": "^4.14.173",
    "@types/node": "^16.9.1",
    "@types/react-helmet": "^6.1.2",
    "tsdocgen": "https://github.com/TsDocGen/tsdocgen.git",
    "tsdocgen-themes": "https://github.com/TsDocGen/tsdocgen-themes.git",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "antd": "^4.16.13",
    "babel-plugin-styled-components": "^1.13.2",
    "gatsby": "^3.13.1",
    "gatsby-plugin-google-analytics": "^3.14.0",
    "gatsby-plugin-manifest": "^3.14.0",
    "gatsby-plugin-mdx": "^2.14.0",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-sitemap": "^4.10.0",
    "gatsby-plugin-styled-components": "^4.14.0",
    "gatsby-source-filesystem": "^3.14.0",
    "husky": "^7.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.3.1",
    "ts-node": "^10.2.1"
  },
  "peerDependencies": {
    "gatsby": "^3.13.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tsdocgen": "https://github.com/TsDocGen/tsdocgen.git",
    "tsdocgen-themes": "https://github.com/TsDocGen/tsdocgen-themes.git"
  }
}
